{
	"name": "ymovie",
	"version": "1.0.0",
	"description": "ymovie",
	"engines": {
		"node": "12.x"
	},
	"main": "server/index.js",
	"scripts": {
		"client-build": "node client/build/prepare.js && npm run client-compile",
		"client-compile": "tsc --project client/tsconfig.json",
		"server-build": "node server/build/prepare.js",
		"build": "node build/prepare.js && npm run client-build && npm run server-build",
		"start": "npm run build && node dist/server/index.js",
		"start-debug": "npm run build && node --inspect dist/server/index.js debug"
	},
	"license": "MIT",
	"dependencies": {
		"@babel/core": "^7.10.0",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-preset-minify": "^0.5.1",
		"csso": "^4.0.3"
	},
	"devDependencies": {
		"typescript": "^4.1.5"
	}
}
